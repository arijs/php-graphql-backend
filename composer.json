{
	"name": "rhengles/ddsoft-online",
	"type": "project",
	"description": "Set up a modern Siler app by running one command.",
	"license": "MIT",
	"authors": [
		{
			"name": "rhengles",
			"email": "rafael@hengles.net"
		}
	],
	"require": {
		"abraham/twitteroauth": "^2.0",
		"docta/oauth2-mercadolibre": "^1.0",
		"gabordemooij/redbean": "^5.7",
		"hassankhan/config": "^2.1",
		"laminas/laminas-config": "^3.5",
		"laminas/laminas-config-aggregator": "^1.5",
		"laminas/laminas-diactoros": "^2.5.1",
		"laminas/laminas-httphandlerrunner": "^1.4.0",
		"laminas/laminas-stratigility": "^3.3",
		"league/oauth2-facebook": "^2.0",
		"league/oauth2-github": "^3.0",
		"league/oauth2-google": "^4.0",
		"league/oauth2-linkedin": "^5.1",
		"leocavalcante/siler": "^1.7.9",
		"monolog/monolog": "^2.1",
		"riskio/oauth2-auth0": "^2.2",
		"thenetworg/oauth2-azure": "^2.0",
		"webonyx/graphql-php": "^14.6"
	},
	"require-dev": {
		"pestphp/pest": "^0.3",
		"phpunit/phpunit": "^9.3",
		"ramsey/devtools": "^1.4",
		"roave/security-advisories": "dev-master",
		"swoole/ide-helper": "^4.5"
	},
	"extra": {
		"ramsey/devtools": {
			"commands": {
				"lint": {
					"override": true,
					"script": "phpcs --cache=./var/cache/phpcs.cache"
				},
				"lint:fix": {
					"override": true,
					"script": "phpcbf --cache=./var/cache/phpcs.cache"
				},
				"test:unit": {
					"override": true,
					"script": "pest"
				}
			}
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Test\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"start": [
			"Composer\\Config::disableProcessTimeout",
			"php -S localhost:8182 srv/sapi.php"
		],
		"swoole": [
			"Composer\\Config::disableProcessTimeout",
			"php srv/swoole.php"
		]
	}
}
